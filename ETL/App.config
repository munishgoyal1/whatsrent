<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="ROOT_DIR" value="D:\WhatsRent\Data\Rent\CommonFloor\"/>
  </appSettings>
  <connectionStrings>
    <add name="OldConnectionString" providerName="System.Data.SqlClient" connectionString="Data Source=MUNISH-LENOVO;Initial Catalog = WhatsRent; Integrated Security=SSPI;min pool size=5;max pool size=50;"/>
  <add name="WhatsRentConnectionString" providerName="System.Data.SqlClient" connectionString="Data Source = MUNISH-LENOVO; Initial Catalog = WhatsRent; Integrated Security = True"/>
  </connectionStrings>
  <system.diagnostics>
    <sources>
      <source name="System.Net" tracemode="includehex" maxdatasize="1024">
        <listeners>
          <add name="System.Net"/>
        </listeners>
      </source>
      <source name="System.Net.Sockets">
        <listeners>
          <add name="System.Net"/>
        </listeners>
      </source>
      <source name="System.Net.Cache">
        <listeners>
          <add name="System.Net"/>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="System.Net" value="Verbose"/>
      <add name="System.Net.Sockets" value="Verbose"/>
      <add name="System.Net.Cache" value="Verbose"/>
    </switches>
    <sharedListeners>
      <add name="System.Net" type="System.Diagnostics.TextWriterTraceListener" initializeData="c:\network.log"/>
    </sharedListeners>
    <trace autoflush="true"/>
  </system.diagnostics>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
